  "dependencies": {
    "vue": "3.5.17"
  },
  "devDependencies": {
    "@types/bun": "1.2.17",
    "@types/html-minifier-terser": "7.0.2",
    "@vitejs/plugin-vue": "6.0.0",
    "@vue/tsconfig": "0.7.0",
    "html-minifier-terser": "7.2.0",
    "typescript": "5.8.3",
    "vite": "npm:rolldown-vite@latest",
    "vite-ssg": "28.0.0",
    "vue-tsc": "2.2.10"
  }
}
