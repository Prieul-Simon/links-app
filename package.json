{
  "name": "links-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "            bunx --bun vite",
    "typecheck-watch": "vue-tsc -p tsconfig.app.json --watch",
    "typecheck": "      vue-tsc -p tsconfig.app.json",
    "bundle": "         bunx --bun vite build",
    "bundle-ssg": "     bunx --bun vite-ssg build -c vite-ssg.config.ts",
    "build": "          bun run typecheck && bun run bundle-ssg",
    "preview": "        bunx --bun vite preview",
    "preview-ssg": "    bunx --bun vite preview --outDir dist-ssg"
  },
  "dependencies": {
    "vue": "3.5.17"
  },
  "devDependencies": {
    "@types/bun": "1.2.17",
    "@vitejs/plugin-vue": "6.0.0",
    "@vue/tsconfig": "0.7.0",
    "typescript": "5.8.3",
    "vite": "7.0.0",
    "vite-ssg": "28.0.0",
    "vue-tsc": "2.2.10"
  }
}
